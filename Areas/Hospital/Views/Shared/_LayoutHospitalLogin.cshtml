<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <!-- Favicons-->
    <link rel="shortcut icon" href="~/css/PatientModuleCSS/img/favicon.ico" type="image/x-icon">
    <link rel="apple-touch-icon" type="image/x-icon" href="~/css/PatientModuleCSS/img/apple-touch-icon-57x57-precomposed.png">
    <link rel="apple-touch-icon" type="image/x-icon" sizes="72x72" href="~/css/PatientModuleCSS/img/apple-touch-icon-72x72-precomposed.png">
    <link rel="apple-touch-icon" type="image/x-icon" sizes="114x114" href="~/css/PatientModuleCSS/img/apple-touch-icon-114x114-precomposed.png">
    <link rel="apple-touch-icon" type="image/x-icon" sizes="144x144" href="~/css/PatientModuleCSS/img/apple-touch-icon-144x144-precomposed.png">

    <!-- GOOGLE WEB FONT -->
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800&display=swap" rel="stylesheet">

    <!-- BASE CSS -->
    <link href="~/css/PatientModuleCSS/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/PatientModuleCSS/css/style.css" rel="stylesheet">
    <link href="~/css/PatientModuleCSS/css/menu.css" rel="stylesheet">
    <link href="~/css/PatientModuleCSS/css/vendors.css" rel="stylesheet">
    <link href="~/css/PatientModuleCSS/css/icon_fonts/css/all_icons_min.css" rel="stylesheet">

    <!-- YOUR CUSTOM CSS -->
    <link href="~/css/PatientModuleCSS/css/custom.css" rel="stylesheet">

    <!-- Modernizr -->
    <script src="~/css/PatientModuleCSS/js/modernizr.js"></script>
</head>
<body>
    <div>


        <div id="preloader" class="Fixed">
            <div data-loader="circle-side"></div>
        </div>
        <!-- /Preload-->

        <div id="page">
            <header class="header_sticky">

                <div class="container">
                    <div class="row">
                        <div class="col-lg-3 col-6">
                            <div id="logo_home">
                                <h1><a href="/Patient/home" title="Findoctor">Findoctor</a></h1>
                            </div>
                        </div>
                       
                    </div>
                </div>
                <!-- /container -->
            </header>
            <!-- /header -->

            @RenderBody()

  
        </div>
        <!-- page -->

        <div id="toTop"></div>
        <!-- Back to top button -->
        <!-- COMMON SCRIPTS -->
        <script src="~/css/PatientModuleCSS/js/jquery-3.6.0.min.js"></script>
        <script src="~/css/PatientModuleCSS/js/common_scripts.min.js"></script>
        <script src="~/css/PatientModuleCSS/js/functions.js"></script>

     



    </div>
</body>
</html>
